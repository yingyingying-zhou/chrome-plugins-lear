{
  "manifest_version":2,
  "name":"change-bg",
  "description":"这是个改变浏览器背景的测试插件",
  "version":"1.0.0",
  "browser_action":
	{
		"default_icon": "img/icon.png",
		"default_title": "这是一个示例Chrome插件",
		"default_popup": "popup.html"
	},
  "content_scripts": [

    {
    
    "matches": ["<all_urls>"],
    
    "css": ["main.css"],
    "js": ["main.js"]
    
    }
    
    ]

}